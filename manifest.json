{
  "manifest_version": 3,
  "name": "GoodbyePaywall",
  "version": "0.2",
  "description": "Selecciona un elemento de la p√°gina y elimina el bloque en su totalidad.",
  "permissions": ["scripting", "activeTab", "tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["popup/GoodbyePaywall.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
